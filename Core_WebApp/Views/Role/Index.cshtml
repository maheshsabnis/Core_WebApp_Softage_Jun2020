@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>


<a asp-action="Create">Create New</a>

<h1>List of Roles</h1>  

<table class="table table-bordered table-striped table-dark">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
        </tr>
        }
    </tbody>
</table>   